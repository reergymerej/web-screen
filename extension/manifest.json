{
  "name": "Web Screen",
  "version": "1.0",
  "description": "Try and hide stuff you don't want to see while browsing.",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
